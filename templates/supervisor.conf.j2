[program:{{ helga_nick }}]
command={{ helga_home }}/bin/helga --settings={{ helga_home }}/bin/custom_settings.py
directory={{ helga_home }}/bin
stderr_logfile=/var/log/supervisor/{{ helga_nick }}.log
stdout_logfile=/var/log/supervisor/{{ helga_nick }}-stdout.log
stopsignal=INT
stopwaitsecs=16
user={{ ansible_ssh_user }}
